{
    "name": "WebRetriever",
    "homepage": "https://vingester.app",
    "version": "2.8.0",
    "description": "Ingest Web Contents as Video Streams",
    "author": {
        "name": "ShowSysDan"
    },
    "keywords": [
        "web",
        "contents",
        "video",
        "audio",
        "stream",
        "capture",
        "ndi"
    ],
    "license": "GPL-3.0-only",
    "private": true,
    "devDependencies": {
        "cross-env": "7.0.3",
        "cross-zip": "4.0.0",
        "electron": "18.0.4",
        "electron-builder": "23.0.3",
        "electron-notarize": "1.2.1",
        "eslint": "8.13.0",
        "eslint-config-standard": "17.0.0-1",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-node": "11.1.0",
        "glob": "8.0.1",
        "htmllint-cli": "0.0.7",
        "nodemon": "2.0.15",
        "npm-run-all": "4.1.5",
        "patch-package": "6.4.7",
        "prompt-password": "1.2.0",
        "shell": "0.9.4",
        "stylelint": "14.6.1",
        "stylelint-config-standard": "25.0.0"
    },
    "dependencies": {
        "@discordjs/opus": "0.7.0",
        "@fortawesome/fontawesome-free": "6.1.1",
        "@rse/ffmpeg": "1.1.0",
        "adm-zip": "0.5.9",
        "bluebird": "3.7.2",
        "clone": "2.1.2",
        "dayjs": "1.11.1",
        "dsig": "1.1.7",
        "ebml": "3.0.0",
        "electron-context-menu": "3.1.2",
        "electron-log": "4.4.6",
        "electron-store": "8.0.1",
        "execa": "5.1.1",
        "express": "^4.22.1",
        "got": "11.8.3",
        "grandiose": "rse/grandiose#cf09bb84",
        "js-yaml": "4.1.0",
        "less": "4.1.2",
        "mkdirp": "1.0.4",
        "moment": "2.29.2",
        "multer": "^1.4.5-lts.1",
        "pcm-convert": "1.6.5",
        "pure-uuid": "1.6.2",
        "rimraf": "3.0.2",
        "throttle-debounce": "4.0.1",
        "tmp": "0.2.1",
        "typopro-web": "4.2.6",
        "update-helper": "1.1.5",
        "vue": "3.2.33",
        "vue-next-select": "2.10.4",
        "vue-tippy": "6.0.0-alpha.44",
        "vue3-perfect-scrollbar": "1.6.0",
        "which": "2.0.2"
    },
    "main": "vingester-main.js",
    "upd": [
        "!execa",
        "!got",
        "!dsig"
    ],
    "build": {
        "appId": "com.showsysdan.apps.webretriever",
        "productName": "Vingester",
        "asar": true,
        "asarUnpack": [
            "node_modules/@rse/ffmpeg/**",
            "node_modules/grandiose/**",
            "node_modules/@discordjs/opus/**"
        ],
        "removePackageScripts": true,
        "compression": "normal",
        "directories": {
            "output": "dist"
        },
        "mac": {
            "target": [
                {
                    "target": "dir"
                }
            ],
            "category": "com.engelschall.apps",
            "identity": null,
            "icon": "vingester-icon.png",
            "files": [
                "!node_modules/electron-builder/**",
                "!node_modules/electron-notarize/**",
                "!node_modules/shelljs/**",
                "!node_modules/eslint*/**",
                "!node_modules/@eslint*/**",
                "!node_modules/stylelint*/**",
                "!node_modules/@stylelint*/**",
                "!node_modules/htmllint*/**",
                "!node_modules/cross-env/**",
                "!node_modules/cross-zip/**",
                "!node_modules/electron/dist/**",
                "!node_modules/dmg-builder/**",
                "!node_modules/app-builder-bin/**",
                "!node_modules/app-builder-lib/**",
                "!node_modules/7zip-bin/**",
                "!node_modules/npm-install-fetch/**"
            ]
        },
        "win": {
            "target": [
                {
                    "target": "portable"
                }
            ],
            "icon": "vingester-icon.png",
            "files": [
                "!*.ai",
                "!node_modules/electron-builder/**",
                "!node_modules/electron-notarize/**",
                "!node_modules/shelljs/**",
                "!node_modules/eslint*/**",
                "!node_modules/@eslint*/**",
                "!node_modules/stylelint*/**",
                "!node_modules/@stylelint*/**",
                "!node_modules/htmllint*/**",
                "!node_modules/cross-env/**",
                "!node_modules/cross-zip/**",
                "!node_modules/electron/dist/**",
                "!node_modules/dmg-builder/**",
                "!node_modules/app-builder-bin/**",
                "!node_modules/app-builder-lib/**",
                "!node_modules/7zip-bin/**",
                "!node_modules/npm-install-fetch/**"
            ]
        },
        "linux": {
            "category": "AudioVideo",
            "target": [
                {
                    "target": "AppImage"
                }
            ],
            "executableName": "Vingester",
            "icon": "vingester-icon.png",
            "files": [
                "!*.ai",
                "!node_modules/electron-builder/**",
                "!node_modules/electron-notarize/**",
                "!node_modules/shelljs/**",
                "!node_modules/eslint*/**",
                "!node_modules/@eslint*/**",
                "!node_modules/stylelint*/**",
                "!node_modules/@stylelint*/**",
                "!node_modules/htmllint*/**",
                "!node_modules/cross-env/**",
                "!node_modules/cross-zip/**",
                "!node_modules/electron/dist/**",
                "!node_modules/dmg-builder/**",
                "!node_modules/app-builder-bin/**",
                "!node_modules/app-builder-lib/**",
                "!node_modules/7zip-bin/**",
                "!node_modules/npm-install-fetch/**"
            ]
        },
        "portable": {
            "useZip": true,
            "artifactName": "Vingester.exe"
        }
    },
    "scripts": {
        "postinstall": "patch-package --patch-dir package.d && electron-builder install-app-deps",
        "lint": "eslint --config etc-eslint.yaml --ext js *.js && stylelint --config etc-stylelint.yaml *.css && htmllint --rc etc-htmllint.json *.html",
        "start": "cross-env DEBUG=1 nodemon --exec \"npx electron --trace-warnings .\" --watch . --ext html,yaml,css,js",
        "start-debug": "cross-env DEBUG=2 nodemon --exec \"npx electron --inspect --trace-warnings .\"  --watch . --ext html,yaml,css,js",
        "start-prod": "electron .",
        "package": "node npm-package.js",
        "clean": "rimraf dist",
        "distclean": "rimraf dist node_modules"
    }
}
